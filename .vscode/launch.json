{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Django",
          "type": "debugpy",
          "request": "launch",
          "program": "${workspaceFolder}/manage.py",
          "args": [
              "runserver",
              "--noreload"
          ],
          "django": true,
          "console": "integratedTerminal",
          "env": {
              "PYTHONUNBUFFERED": "1"
          },
          "justMyCode": true
      }
  ]
}
